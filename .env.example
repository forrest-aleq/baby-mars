# Baby MARS Environment Variables
# =================================

# Required: Anthropic API Key
ANTHROPIC_API_KEY=sk-ant-...

# Database (optional - will use in-memory if not set)
DATABASE_URL=postgresql://mars:mars@localhost:5432/baby_mars

# Stargate (optional - enables REAL tool execution)
# 295 capabilities across 20+ platforms when enabled
STARGATE_URL=http://localhost:8001
STARGATE_API_KEY=your-stargate-api-key

# Logging
LOG_LEVEL=INFO
LOG_JSON=false

# Server
HOST=0.0.0.0
PORT=8000

# Auth (optional - allows all if not set)
BABY_MARS_API_KEYS=
